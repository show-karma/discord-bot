    "@types/lodash": "^4.14.182",
    "aws-sdk": "^2.1174.0",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "discord-api-types": "^0.34.0",
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "ethers": "^5.7.0",
    "express": "^4.18.1",
